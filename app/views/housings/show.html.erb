<h1>閲覧</h1>
<p>物件名：<%= @housing.build %></p>
<p>賃料：<%= @housing.rent %></p>
<p>住所：<%= @housing.add %></p>
<p>築年数：<%= @housing.year %></p>
<p>備考：<%= @housing.remarks %></p>
<div class="borderLine"></div>
<% count = 1 %>
<% @housing.near_stations.each do |near| %>
  <h3>最寄駅<%= count %></h3>
  <p>路線名：<%= near.line %></p>
  <p>駅名：<%= near.station %></p>
  <p>徒歩分数：<%= near.walk %></p>
  <% count += 1 %>
<% end %>
<%= link_to '編集', edit_housing_path(@housing.id) %>｜<%= link_to '戻る', :back %>
